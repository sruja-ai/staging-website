// examples/demo_datastores_queues.sruja
// Queues and DataStore Example


system = kind "System"
container = kind "Container"
queue = kind "Queue"
database = kind "Database"



service = system "Service" {
  api = container "API"
  mq = queue "Message Queue"
  db = database "Database"
}

service.api -> service.mq "publishes messages"
service.api -> service.db "reads data"
service.api -> service.db "writes data"



view index {
  title "Queues and DataStore"
  include *
}

